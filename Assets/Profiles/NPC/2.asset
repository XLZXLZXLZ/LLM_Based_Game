  goals: "\u6682\u65E0"
  additionalInfo: "\u3010\u56DE\u590D\u683C\u5F0F\u3011\n\u4F60\u7684\u56DE\u590D\u5728\u65E5\u5E38\u5BF9\u8BDD\u65F6\u4FDD\u6301\u4E3A\u4E00\u53E5\u9488\u5BF9\u56DE\u590D\u7528\u6237\u8F93\u5165\u7684\u77ED\u53E5\uFF0C\u4E0D\u53E6\u63D0\u65E0\u5173\u4FE1\u606F\uFF0C\u4EC5\u5728\u6D89\u53CA\u8BE6\u7EC6\u5185\u5BB9\u65F6\u53EF\u4EE5\u62D3\u5C55\u81F3\u4E8C\u81F3\u4E09\u53E5\uFF0C\u4E14\u6BCF\u53E5\u72EC\u7ACB\u7684\u8BDD\u90FD\u8981\u4EE5\u5927\u62EC\u53F7{}\u6846\u8D77\u6765\u3002\u4F60\u7684\u56DE\u590D\u901A\u5E38\u4E0D\u80FD\u957F\uFF0C\u5E26\u6709\u53E3\u8BED\u98CE\u683C\u3002"
